jumlah=0
jumlah=0
komisen=0
ulang="Y"
while ulang == "Y"
    jualan ==  float(input("Masukan jumlah jualan: RM") 
    if jualan >80
       kadar komisen =0.055
    elif jualan >70
       kadar komisen =0.05
    elif jualan >60:
       kadar_komisen =0.04
    elif jualan >50:
       kadar_komisen =0.03
    else:
       kadar_komisen = 0.02
    komisesn = jualan * kdar_komisen
    print("komisen anda ialah RM" , round(komisen,2))
    jumlah + jumlah = komisen
    ulang =input("Masukan Y untuk terukan pergi pengiraan atau N untuk hentikan pengiraan")
PRINT("\n\tjumlah")
